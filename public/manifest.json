{
  "manifest_version": 3,
  "name": "AC",
  "version": "0.1",
  "description": "Deez",

  "permissions": ["tabs", "activeTab", "tabCapture", "scripting"],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content_script.js"]
    }
  ]
}
